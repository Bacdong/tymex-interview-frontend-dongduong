{
  "name": "tymex-interview-frontend-dongduong",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.1.13",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:staged": "vitest --run",
    "prepare": "husky"
